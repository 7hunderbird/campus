<div class="row-fluid">
	<div class="span4">
		<a href="<%= user_path(current_user) %>">
		  <%= gravatar_for current_user, size: 52 %>
		</a>
	</div>
	<div class="span8">
		<h4>
		  <%= current_user.email %>
		</h4>
	</div>
</div>
<div class="row-fluid">
	<%= link_to "view my profile", current_user %><br>
	<%= pluralize(current_user.study_plans.count, "study_plans") %>
</div>
